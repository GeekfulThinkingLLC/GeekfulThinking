<div class="title-bar" data-responsive-toggle="animated-menu" data-hide-for="medium">
  <div class="title-bar-title mobile-menu-text">
    <%= link_to "Geekful Thinking LLC", root_path %>    
  </div>
  <button class="fa fa-bars float-right" type="button" data-toggle="animated-menu"></button>
</div>

<div class="top-bar" id="animated-menu" data-animate="slide-in-left slide-out-right">
  <div class="top-bar-left">
    <ul class="menu medium-horizontal hide-for-small-only">
      <li class="nav-logo"><%= link_to "Geekful Thinking LLC", root_path %></li>
    </ul>
  </div>
  <div class="top-bar-right">
    <ul class="menu nav-links">
      <li><%= link_to "About", about_path %></li>
      <li><%= link_to "Social Mission", mission_path %></li>
      <li><%= link_to "Consultations", new_consultation_path %></li>
      <% if logged_in?(:admin) %>
        <li><%= link_to "Leads Generated", consultations_path %></li>
        <li><%= link_to "Logout", destroy_user_session_path, method: :delete %></li>
      <% end %>
    </ul>
  </div>
</div>